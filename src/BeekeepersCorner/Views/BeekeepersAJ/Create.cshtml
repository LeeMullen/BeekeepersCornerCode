@{
    Layout = "~/Views/Shared/_LayoutAngular.cshtml";
}



<h2>Create Beekeeper (AJ)</h2>


    <div class="form-horizontal" ng-controller="CreateBeekeeperController">
        <hr />
        

        <div class="form-group">
            <div class="control-label col-md-2">First Name:</div>
            <div class="col-md-10">
                <input type="text" ng-model="beekeeper.FirstName">
            </div>
        </div>
        <div class="form-group">
            <div class="control-label col-md-2">Last Name:</div>
            <div class="col-md-10">
                <input type="text" ng-model="beekeeper.LastName">
            </div>
        </div>
        <div class="form-group">
            <div class="control-label col-md-2">Address:</div>
            <div class="col-md-10">
                <input type="text" ng-model="beekeeper.Address">
            </div>
        </div>
        <div class="form-group">
            <div class="control-label col-md-2">City:</div>
            <div class="col-md-10">
                <input type="text" ng-model="beekeeper.City">
            </div>
        </div>
        <div class="form-group">
            <div class="control-label col-md-2">State:</div>
            <div class="col-md-10">
                <input type="text" ng-model="beekeeper.State">
            </div>
        </div>
        <div class="form-group">
            <div class="control-label col-md-2">Zipcode:</div>
            <div class="col-md-10">
                <input type="text" ng-model="beekeeper.Zipcode">
            </div>
        </div>
        <div class="form-group">
            <div class="control-label col-md-2">Phone:</div>
            <div class="col-md-10">
                <input type="text" ng-model="beekeeper.Phone">
            </div>
        </div>
        <div class="form-group">
            <div class="control-label col-md-2">Email:</div>
            <div class="col-md-10">
                <input type="text" ng-model="beekeeper.Email">
            </div>
        </div>
        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <button ng-click="save()" class="btn btn-default">Save</button>
            </div>
        </div>
    </div>

<div>
    @Html.ActionLink("Back to Beekeepers List (AJ)", "BeekeepersAJ", "BeekeepersAJ")
</div>
